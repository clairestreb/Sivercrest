@{
    Layout = "~/Views/Shared/_LayoutNoBar.cshtml";
    ViewBag.Title = "Password Reset Successful!";
}
<div class="container">
    <hgroup class="title ">
        <h2 style="font-family:Goudy">@ViewBag.Title</h2>
    </hgroup>

    <p style="font-family:Goudy">
        Your password has been reset. Please click @Html.ActionLink(" here ", "Login", "Account", routeValues: null, htmlAttributes: new { id = "loginLink" }) to login.
    </p>
</div>
