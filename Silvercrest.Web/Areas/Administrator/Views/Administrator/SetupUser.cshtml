@model Silvercrest.ViewModels.SetupUserViewModel
@{
    ViewBag.Title = "SetupUser";
}
<link href="~/Content/index.css" rel="stylesheet" />
<section id="breadcrumbs">
    <div class="container">
        <div class="row">
            <div class="col-free pull-right">
                <img src="~/img/return-arrow.png" alt="">
                <a href="/Administrator/Administrator/Index">Back to Admin Tasks</a>
            </div>

        </div>
    </div>
</section>	<section id="title">
    <div class="container">
        <div class="row">
            <h2>User Admin Tasks</h2>
            <h3>NEW USER SET-UP</h3>
        </div>
    </div>
</section>


<section id="new-user">
    <div class="container pt60">
        <div class="box">
            <div class="form-group edge-to-edge pb30">
                <div class="col-xs-12 col-sm-5">
                    <input id="email" type="text" class="form-control" placeholder="Email">
                </div>
                <div  class="col-xs-12 col-sm-5">
                    <input type="text" id="contact" class="form-control" placeholder="Contact">
                </div>
                <div class="col-xs-12 col-sm-2 pull-right">
                    <button type="submit" name="submit" id="submit" class="btn btn-red pull-right">Submit</button>
                </div>
            </div>
        </div>
    </div>
</section>


